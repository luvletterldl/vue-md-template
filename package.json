{
  "name": "mind",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 9876 --open",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
